{
  "functions": {
    "app/api/intake/route.ts": {
      "maxDuration": 30
    },
    "app/api/reservations/route.ts": {
      "maxDuration": 30
    },
    "app/api/admin/line/broadcast/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/send-scheduled/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/process-steps/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/generate-reminders/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/ai-reply/route.ts": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/cron/ai-reply",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/send-scheduled",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/process-steps",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/collect-line-stats",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/generate-reminders?offset=0",
      "schedule": "30 23 * * *"
    },
    {
      "path": "/api/cron/generate-reminders?offset=1",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/cron/health-report",
      "schedule": "0 9 * * *"
    }
  ]
}
